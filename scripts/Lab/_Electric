;;; ---------------------------------------------------------------------------
;;; Declarations
:local int i
; :global int current_wrap

;;; ---------------------------------------------------------------------------
;;; Impulse
key.e()

;;; ---------------------------------------------------------------------------
;;; Conditions
isopen("laboratory")

;;; ---------------------------------------------------------------------------
;;; Macros

;;; ---------------------------------------------------------------------------
;;; Actions
    ; {requireMinRestSecond1("Electric", 300.)}
    ; {requireMinRestSecond2("Electric")}

LOOP_START:
    {gridClick(0.57, 0.68, 0.57, 0.42, 1, 20, i)}
    {relclick(0.77, 0.47)}
    i = i + 1

    gotoif(LOOP_START, i < 20)
