
;;; ---------------------------------------------------------------------------
;;; Definitions
:local int a
:local double y
:global double _2
:local double x
:global double _1

;;; ---------------------------------------------------------------------------
;;; Actions
    x = _1
    y = _2

    a = 0
START:
    click(vec(x, y))
    wait(0.2)

    a = a + 1
    gotoif(START, a < 64)
