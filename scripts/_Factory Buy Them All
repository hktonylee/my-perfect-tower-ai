:global double _2
:global double _1
:local double a

click(vec(width.d() * 0.96, height.d() * 0.59))
scrollbar(vec(width.d() * 0.91, height.d() * 0.69), -1.0, 0.5)
a = 0.0
_1 = width.d() * (((0.85 - 0.5) * ((a % 3.0) / 2.0)) + 0.5)
_2 = height.d() * (((0.82 - 0.5) * (i2d(d2i(a) / 3) / 5.0)) + 0.5)
execute("Slow Click 64 Times")
a = a + 1.0
gotoif(4, a < 16.0)
wait(3.0)
stop("Slow Click 64 Times")
