
;;; ---------------------------------------------------------------------------
;;; Declarations
:global int dig_index
:local int x

;;; ---------------------------------------------------------------------------
;;; Actions
x = dig_index
dig((x + 0) % 4, (x + 0) / 4)
dig((x + 1) % 4, (x + 1) / 4)
dig((x + 2) % 4, (x + 2) / 4)
dig((x + 3) % 4, (x + 3) / 4)
