
;;; ---------------------------------------------------------------------------
;;; Declarations
:local int i
:local double x
:local double y
:global double _1
:global double _2

;;; ---------------------------------------------------------------------------
;;; Actions
    x = _1
    y = _2

    i = 0
START:
    click(vec(x, y))
    wait(0.2)

    i = i + 1
    gotoif(START, i < 64)
