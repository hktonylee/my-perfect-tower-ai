;;; ---------------------------------------------------------------------------
;;; Definitions
:global int _1
:local int i

;;; ---------------------------------------------------------------------------
;;; Actions
    i = 0
LOOP_START:
    ; Randomly pick one of the Tier 1~5
    _1 = int.rnd(1, 5)

    executesync("Factory/_Make One Dust")
    i = i + 1
    gotoif(LOOP_START, i < 5)
